{
  "expo": {
    "name": "GroundSchool AI",
    "slug": "groundschool-ai",
    "version": "1.1.0",
    "sdkVersion": "51.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "orientation": "portrait",
    "plugins": [
      "sentry-expo"
    ],
    "web": {
      "favicon": "./assets/favicon.png",
      "manifest": {
        "name": "GroundSchool AI",
        "short_name": "GSAI",
        "start_url": ".",
        "display": "standalone",
        "background_color": "#F9FAFB",
        "theme_color": "#4F46E5",
        "icons": [
          { "src": "./assets/icon.png", "sizes": "192x192", "type": "image/png" },
          { "src": "./assets/icon.png", "sizes": "512x512", "type": "image/png" }
        ]
      },
      "serviceWorker": {
        "src": "./service-worker.js"
      }
    }
  }
}
